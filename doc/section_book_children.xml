        <simplelist type='inline'>
            <member><link linkend="element_appendix">appendix</link></member>
            <member><link linkend="element_article">article</link></member>
            <member><link linkend="element_bibliography">bibliography</link></member>
            <member><link linkend="element_bookinfo">bookinfo</link></member>
